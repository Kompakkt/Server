{
  "name": "objectsrepositoryserver",
  "version": "0.0.1",
  "description": "Server for ObjectsRepository",
  "main": "dist/server.js",
  "scripts": {
    "start": "ts-node src/server.ts",
    "build": "tsc -p .",
    "dev": "./node_modules/nodemon/bin/nodemon.js",
    "report": "jscpd -k 10 -l 2 src/",
    "lint": "eslint -c .eslintrc.js ./src/**/*.ts",
    "prepare": "husky install",
    "prettier": "prettier --config .prettierrc.json 'src/**/*.{ts,html,scss}' --write",
    "postprepare": "git submodule update --remote --merge"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DH-Cologne/ObjectsRepositoryServer.git"
  },
  "jscpd": {
    "min-lines": 2,
    "min-tokens": 10,
    "reporters": [
      "html",
      "badge"
    ],
    "ignore": [
      "node_modules"
    ],
    "gitignore": true
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/DH-Cologne/ObjectsRepositoryServer/issues"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run prettier"
    }
  },
  "homepage": "https://github.com/DH-Cologne/ObjectsRepositoryServer#readme",
  "dependencies": {
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "connect-redis": "^6.0.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "corser": "^2.0.1",
    "deepmerge": "^4.2.2",
    "express": "^4.17.1",
    "express-session": "^1.17.2",
    "flatten": "^1.0.3",
    "fs-extra": "^10.0.0",
    "imagemin": "^7.0.1",
    "imagemin-pngquant": "^8.0.0",
    "ioredis": "^4.27.7",
    "klaw-sync": "^6.0.0",
    "mongodb": "^4.0.1",
    "multer": "^1.4.2",
    "nodemailer": "^6.6.3",
    "nodemon": "^2.0.12",
    "object-hash": "^2.2.0",
    "passport": "^0.4.1",
    "passport-ldapauth": "^3.0.1",
    "passport-local": "^1.0.0",
    "response-time": "^2.3.2",
    "shrink-ray-current": "^4.1.3",
    "slugify": "^1.6.0",
    "socket.io": "^4.1.3",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.1",
    "@types/compression": "1.7.1",
    "@types/connect-redis": "^0.0.17",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-serve-static-core": "^4.17.24",
    "@types/express-session": "^1.17.4",
    "@types/fs-extra": "^9.0.12",
    "@types/imagemin": "^7.0.1",
    "@types/ioredis": "^4.26.6",
    "@types/klaw": "^3.0.2",
    "@types/klaw-sync": "^6.0.1",
    "@types/multer": "^1.4.7",
    "@types/node": "^14.17.7",
    "@types/nodemailer": "^6.4.4",
    "@types/object-hash": "^2.1.1",
    "@types/passport": "^1.0.7",
    "@types/passport-local": "^1.0.34",
    "@types/response-time": "^2.3.5",
    "@typescript-eslint/eslint-plugin": "^4.29.0",
    "@typescript-eslint/parser": "^4.29.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^7.0.1",
    "jscpd": "^3.3.26",
    "jscpd-badge-reporter": "^1.1.3",
    "jscpd-html-reporter": "^2.0.1",
    "prettier": "^2.3.2"
  },
  "optionalDependencies": {
    "node-zopfli-es": "~1.0.7"
  }
}
