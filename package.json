{
  "name": "objectsrepositoryserver",
  "version": "0.0.1",
  "description": "Server for ObjectsRepository",
  "main": "dist/server.js",
  "scripts": {
    "start": "ts-node src/server.ts",
    "build": "tsc -p .",
    "dev": "./node_modules/nodemon/bin/nodemon.js",
    "report": "jscpd -k 10 -l 2 src/",
    "lint": "eslint -c .eslintrc.js ./src/**/*.ts",
    "prettier": "prettier --config .prettierrc.json 'src/**/*.{ts,html,scss}' --write",
    "postprepare": "git submodule update --remote --merge"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DH-Cologne/ObjectsRepositoryServer.git"
  },
  "jscpd": {
    "min-lines": 2,
    "min-tokens": 10,
    "reporters": [
      "html",
      "badge"
    ],
    "ignore": [
      "node_modules"
    ],
    "gitignore": true
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/DH-Cologne/ObjectsRepositoryServer/issues"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run prettier"
    }
  },
  "homepage": "https://github.com/DH-Cologne/ObjectsRepositoryServer#readme",
  "dependencies": {
    "axios": "^0.20.0",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "connect-redis": "^6.0.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "corser": "^2.0.1",
    "deepmerge": "^4.2.2",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "flatten": "^1.0.3",
    "fs-extra": "^9.0.1",
    "imagemin": "^7.0.1",
    "imagemin-pngquant": "^9.0.0",
    "ioredis": "^4.17.3",
    "klaw-sync": "^6.0.0",
    "mongodb": "^3.6.0",
    "multer": "^1.4.2",
    "nodemailer": "^6.4.11",
    "nodemon": "^2.0.4",
    "object-hash": "^2.0.3",
    "passport": "^0.4.1",
    "passport-ldapauth": "^2.1.4",
    "passport-local": "^1.0.0",
    "response-time": "^2.3.2",
    "shrink-ray-current": "^4.1.2",
    "slugify": "^1.4.5",
    "socket.io": "^2.3.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2",
    "uuid": "^8.3.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/compression": "1.7.0",
    "@types/connect-redis": "^0.0.17",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.7",
    "@types/express": "^4.17.7",
    "@types/express-serve-static-core": "^4.17.9",
    "@types/express-session": "^1.17.0",
    "@types/fs-extra": "^9.0.1",
    "@types/imagemin": "^7.0.0",
    "@types/ioredis": "^4.17.3",
    "@types/klaw": "^3.0.1",
    "@types/klaw-sync": "^6.0.0",
    "@types/mongodb": "^3.5.26",
    "@types/multer": "^1.4.7",
    "@types/node": "^14.6.0",
    "@types/nodemailer": "^6.4.0",
    "@types/object-hash": "^1.3.3",
    "@types/passport": "^1.0.4",
    "@types/passport-local": "^1.0.33",
    "@types/response-time": "^2.3.4",
    "@types/socket.io": "^2.1.11",
    "@typescript-eslint/eslint-plugin": "^3.10.0",
    "@typescript-eslint/parser": "^3.10.0",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.2.5",
    "jscpd": "^3.3.16",
    "jscpd-badge-reporter": "^1.1.3",
    "jscpd-html-reporter": "^2.0.1",
    "prettier": "^2.1.0"
  },
  "optionalDependencies": {
    "node-zopfli-es": "~1.0.6"
  }
}
